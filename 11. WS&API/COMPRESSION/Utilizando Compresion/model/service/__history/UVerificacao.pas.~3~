unit UVerificacao;

interface

type
  TVerificar = class
    private
      procedure Post;
      procedure Get;
      procedure Delete;
  end;

implementation

{ TVerificar }

procedure TVerificar.Delete;
begin

end;

procedure TVerificar.Get;
var
  xDadosPessoas: TDadosGerais;
begin
  xDadosPessoas := TDadosGerais.Create('Cargo.json');
  try
    Res.Send<TJSONArray>(xDadosPessoas.JSON);
  finally
    xDadosPessoas.Free;
  end;

