unit UPessoa;

interface

uses
  GBSwagger.Model.Attributes;

type
  TPessoa = class
    private
      FID: Integer;
      FNome: String;
    public
      [SwagProp('Pessoa ID', True)]
      property ID: Integer read FID write FID;

      [SwagProp('Pessoa Nome', True)]
      property Nome: String read FNome write FNome;
  end;

implementation

end.
